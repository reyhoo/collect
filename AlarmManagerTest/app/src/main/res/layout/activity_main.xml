<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <Button
        android:id="@+id/setOnce"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="setOnce" />

    <Button
        android:id="@+id/setRepeat5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="setRepeat5" />

    <Button
        android:id="@+id/setRepeat10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="setRepeat10" />

    <Button
        android:id="@+id/setExact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="setExact" />

    <Button
        android:id="@+id/cancelOnce"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cancelOnce" />

    <Button
        android:id="@+id/cancelRepeat5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cancelRepeat5" />

    <Button
        android:id="@+id/cancelRepeat10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cancelRepeat10" />


    <Button
        android:id="@+id/cancelExact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="cancelExact" />

</LinearLayout>
